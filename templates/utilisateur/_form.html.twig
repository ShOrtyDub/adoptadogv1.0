{{ form_start(form) }}
<div class="pt-3 pb-3">
    {{ form_row(form.nom, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="pt-3 pb-3">
    {{ form_row(form.prenom, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="pt-3 pb-3">
    {{ form_row(form.age, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="pt-3 pb-3">
    {{ form_row(form.telephone, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="pt-3 pb-3">
    {{ form_row(form.photo, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="pt-3 pb-3">
    {{ form_row(form.email, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="pt-3 pb-3">
    {{ form_row(form.password, {
        'attr': {'class': 'form-control-sm'},
        'label_attr': {'class': 'critere'},
    }) }}
</div>

<div class="text-center pt-3 pb-3">
    <button class="recherche">{{ button_label|default('Mettre à jour') }}</button>
    <a class="profil" href="{{ path('app_utilisateur_show', {'id': utilisateur.id}) }}">Revenir au profil</a>
</div>
{{ form_end(form) }}




