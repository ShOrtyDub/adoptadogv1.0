<form name="utilisateur" action="{{ path('app_utilisateur_edit', {'id': utilisateur.id}) }}" method="post">
    <div id="utilisateur" class="mt-5 monprofil champ lignerouge">

{#        Label#}
        <div class="ligneverte">
            <div><label for="utilisateur_email" class="required">Email</label></div>
            <div><label for="utilisateur_roles" class="required">Role</label></div>
            <div><label for="utilisateur_password_first" class="required">Mot de passe</label></div>
            <div><label for="utilisateur_password_second" class="required">Confirmation du mot de passe</label></div>
            <div><label for="utilisateur_nom" class="required">Nom</label></div>
            <div><label for="utilisateur_prenom" class="required">Prénom</label></div>
            <div><label for="utilisateur_age" class="required">Âge</label></div>
            <div><label for="utilisateur_photo">Photo</label></div>
            <div><label for="utilisateur_telephone" class="required">Téléphone</label></div>
            <div><label for="utilisateur_is_valide">Valide</label></div>
        </div>
{#        Input#}
        <div class="ligneverte">
            <div><input type="email" id="utilisateur_email" name="utilisateur[email]" required="required"/></div>
            <div><select id="utilisateur_roles" name="utilisateur[roles]"><option value="ROLE_VISITOR">Visiteur</option></select></div>
            <div><input type="password" id="utilisateur_password_first" name="utilisateur[password][first]" required="required" class="password-field" autocomplete="new-password"/></div>
            <div><input type="password" id="utilisateur_password_second" name="utilisateur[password][second]" required="required" class="password-field" autocomplete="new-password"/></div>
            <div><input type="text" id="utilisateur_nom" name="utilisateur[nom]" required="required"/></div>
            <div><input type="text" id="utilisateur_prenom" name="utilisateur[prenom]" required="required"/></div>
            <div><input type="text" id="utilisateur_age" name="utilisateur[age]" required="required" inputmode="decimal"/></div>
            <div><input type="text" id="utilisateur_photo" name="utilisateur[photo]" maxlength="255"/></div>
            <div><input type="text" id="utilisateur_telephone" name="utilisateur[telephone]" required="required" inputmode="decimal"/></div>
            <div><input type="checkbox" id="utilisateur_is_valide" name="utilisateur[is_valide]" value="1"/></div>
        </div>

    </div>
    {{ form_row(form._token) }}
    <input type="submit" value="Mise à jour">
</form>
